<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Target Name="RunTests">
    <Exec Command="dotnet test Lab2.xUnitTests/Lab2.xUnitTests.csproj" />
  </Target>

  <Target Name="BuildAndRun">
    <Exec Command="dotnet build Lab2/Lab2.csproj" />
    <Exec Command="dotnet run --project Lab2/Lab2.csproj" />
  </Target>

  <Target Name="All" DependsOnTargets="RunTests;BuildAndRun" />
</Project>
